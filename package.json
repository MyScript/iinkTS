{
  "name": "iink-ts",
  "version": "1.0.0-buildVersion",
  "description": "iinkTS is the fastest way to integrate handwriting panel and recognition in your webapp",
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/iink.min.js",
  "module": "dist/iink.esm.js",
  "type": "module",
  "types": "dist/iink.d.ts",
  "keywords": [
    "myscript",
    "javascript",
    "typescript",
    "developer",
    "handwriting",
    "recognition",
    "cloud",
    "iink"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "homepage": "https://myscript.github.io/iinkTS/",
  "repository": "git://github.com/MyScript/iinkTS.git",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test:unit": "jest --config=jest.unit.config.js",
    "test:integration": "jest --config=jest.integration.config.js",
    "build:docs": "typedoc",
    "build:lib": "rollup -c config/rollup.config.build.mjs --sourcemap",
    "build": "npm-run-all lint:fix test:unit build:lib build:docs",
    "dev": "rollup -c config/rollup.config.dev.mjs -w --sourcemap"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "json-css": "^1.5.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-playwright": "^0.18.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^6.4.0",
    "jest-playwright-preset": "^3.0.1",
    "jest-transform-css": "^6.0.1",
    "jest-websocket-mock": "^2.5.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.36.1",
    "postcss": "^8.4.31",
    "rollup": "^4.1.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-svg-import": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.2",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "**/loader-utils": "3.2.1",
    "**/json5": "2.2.3"
  }
}
